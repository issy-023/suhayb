ginalData.name || this.surname !== this.originalData.surname || this.dob !== this.originalData.dob || this.email !== this.originalData.email || this.phone !== this.originalData.phone || this.favParty !== this.originalData.favParty || this.oldPassword || this.newPassword;\n    }\n  },\n  methods: {\n    applyChanges() {\n      if (this.isPasswordValid && !this.oldPassword) {\n        console.log(\"Changes applied\");\n        this.originalData = {\n          name: this.name,\n          surname: this.surname,\n          dob: this.dob,\n          email: this.email,\n          phone: this.phone,\n          favParty: this.favParty\n        };\n      } else {\n        console.log(\"Password does not meet the requirements\");\n      }\n    },\n    deleteAccount() {\n      console.log(\"Account deleted\");\n      this.showDeleteModal = false;\n    },\n    onFileChange(e) {\n      const file = e.target.files[0];\n      this.profilePicture = URL.createObjectURL(file);\n    }\n  },\n  mounted() {\n    this.originalData = {\n      name: this.name,\n      surname: this.surname,\n      dob: this.dob,\n      email: this.email,\n      phone: this.phone,\n      favParty: this.favParty\n    };\n  }\n};","map":{"version":3,"names":["name","data","surname","dob","username","gender","email","phone","favParty","oldPassword","newPassword","profilePicture","oldPasswordClicked","originalData","showDeleteModal","parties","computed","passwordValidations","password","length","uppercase","test","special","noUsername","toLowerCase","includes","noOldPassword","isPasswordValid","hasChanges","methods","applyChanges","console","log","deleteAccount","onFileChange","e","file","target","files","URL","createObjectURL","mounted"],"sources":["/Users/suhayb/Documenten/cuuruuvoovii16/election-frontend/src/components/ProfileComponent.vue"],"sourcesContent":["<template>\n  <div class=\"profile-container\">\n    <div class=\"profile-form\">\n      <h1>Profile</h1>\n      <form @submit.prevent=\"applyChanges\">\n        <div class=\"form-group picture-group\">\n          <label for=\"profile-picture\">Profile Picture:</label>\n          <div class=\"profile-picture-placeholder\">\n            <img v-if=\"profilePicture\" :src=\"profilePicture\" alt=\"Profile Picture\" class=\"profile-picture\" />\n          </div>\n          <input type=\"file\" @change=\"onFileChange\" />\n        </div>\n\n        <div class=\"form-group full-width\">\n          <label for=\"name\">Name:</label>\n          <input\n              type=\"text\"\n              id=\"name\"\n              v-model=\"name\"\n              placeholder=\"Fill in your name\"\n              required\n          />\n        </div>\n\n        <div class=\"form-row\">\n          <div class=\"form-group half-width\">\n            <label for=\"surname\">Surname:</label>\n            <input\n                type=\"text\"\n                id=\"surname\"\n                v-model=\"surname\"\n                placeholder=\"Fill in your surname\"\n                required\n            />\n          </div>\n\n          <div class=\"form-group half-width\">\n            <label for=\"dob\">Date of Birth:</label>\n            <input\n                type=\"date\"\n                id=\"dob\"\n                v-model=\"dob\"\n                required\n            />\n          </div>\n        </div>\n\n        <div class=\"form-group full-width\">\n          <label for=\"email\">E-mail:</label>\n          <input\n              type=\"email\"\n              id=\"email\"\n              v-model=\"email\"\n              placeholder=\"Fill in your email\"\n              required\n          />\n        </div>\n\n        <div class=\"form-group full-width\">\n          <label for=\"phone\">Phone Number:</label>\n          <input\n              type=\"tel\"\n              id=\"phone\"\n              v-model=\"phone\"\n              placeholder=\"Fill in your phone number\"\n              required\n          />\n        </div>\n\n        <div class=\"form-group full-width\">\n          <label for=\"favParty\">Favorite Political Party:</label>\n          <select id=\"favParty\" v-model=\"favParty\" required>\n            <option v-for=\"party in parties\" :key=\"party\">{{ party }}</option>\n          </select>\n        </div>\n\n        <div class=\"form-group full-width\">\n          <label for=\"oldPassword\">Change Password:</label>\n          <input\n              type=\"password\"\n              id=\"oldPassword\"\n              v-model=\"oldPassword\"\n              placeholder=\"Fill in old password\"\n              @focus=\"oldPasswordClicked = true\"\n              required\n          />\n        </div>\n\n\n        <div v-if=\"oldPasswordClicked\" class=\"form-group full-width\">\n          <label for=\"newPassword\">New Password:</label>\n          <input\n              type=\"password\"\n              id=\"newPassword\"\n              v-model=\"newPassword\"\n              placeholder=\"Fill in new password\"\n          />\n          <ul class=\"password-requirements\">\n            <li :class=\"{'valid': passwordValidations.length, 'invalid': !passwordValidations.length}\">\n              At least 8 characters\n            </li>\n            <li :class=\"{'valid': passwordValidations.uppercase, 'invalid': !passwordValidations.uppercase}\">\n              At least one uppercase letter\n            </li>\n            <li :class=\"{'valid': passwordValidations.special, 'invalid': !passwordValidations.special}\">\n              At least one special character\n            </li>\n            <li :class=\"{'valid': passwordValidations.noUsername, 'invalid': !passwordValidations.noUsername}\">\n              Must not contain username\n            </li>\n            <li :class=\"{'valid': passwordValidations.noOldPassword, 'invalid': !passwordValidations.noOldPassword}\">\n              Must not be the same as the old password\n            </li>\n          </ul>\n        </div>\n\n        <div class=\"form-buttons\">\n          <button type=\"button\" class=\"delete-btn\" @click=\"showDeleteModal = true\">Delete Account</button>\n          <button type=\"submit\" class=\"apply-btn\" :disabled=\"!hasChanges\">Apply Changes</button>\n        </div>\n      </form>\n    </div>\n\n    <div v-if=\"showDeleteModal\" class=\"modal\">\n      <div class=\"modal-content\">\n        <h2>Are you sure you want to delete your account?</h2>\n        <p>This action cannot be undone.</p>\n        <div class=\"modal-buttons\">\n          <button class=\"cancel-btn\" @click=\"showDeleteModal = false\">No</button>\n          <button class=\"confirm-btn\" @click=\"deleteAccount\">Yes</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ProfileComponent\",\n  data() {\n    return {\n      name: \"\",\n      surname: \"\",\n      dob: \"\",\n      username: \"\",\n      gender: \"\",\n      email: \"\",\n      phone: \"\",\n      favParty: \"\",\n      oldPassword: \"\",\n      newPassword: \"\",\n      profilePicture: null,\n      oldPasswordClicked: false,\n      originalData: {},\n      showDeleteModal: false,\n      parties: [\n        \"VVD\",\n        \"D66\",\n        \"GROENLINKS / Partij van de Arbeid (PvdA)\",\n        \"PVV (Partij voor de Vrijheid)\",\n        \"CDA\",\n        \"SP (Socialistische Partij)\",\n        \"Forum voor Democratie\",\n        \"Partij voor de Dieren\",\n        \"ChristenUnie\",\n        \"Volt\",\n        \"JA21\",\n        \"Staatkundig Gereformeerde Partij (SGP)\",\n        \"DENK\",\n        \"50PLUS\",\n        \"BBB\",\n        \"BIJ1\",\n        \"Piratenpartij – De Groenen\",\n        \"BVNL / Groep Van Haga\",\n        \"Nieuw Sociaal Contract\",\n        \"Splinter\",\n        \"LP (Libertaire Partij)\",\n        \"LEF – Voor de Nieuwe Generatie\",\n        \"Samen voor Nederland\",\n        \"Nederland met een PLAN\",\n        \"PartijvdSport\",\n        \"Politieke Partij voor Basisinkomen\"\n      ]\n    };\n  },\n  computed: {\n    passwordValidations() {\n      const password = this.newPassword;\n      const username = this.username;\n      const oldPassword = this.oldPassword;\n      return {\n        length: password.length >= 8,\n        uppercase: /[A-Z]/.test(password),\n        special: /[!@#$%^&*/,.?\":{}|<>]/.test(password),\n        noUsername: !password.toLowerCase().includes(username.toLowerCase()),\n        noOldPassword: password !== oldPassword\n      };\n    },\n    isPasswordValid() {\n      return (\n          this.passwordValidations.length &&\n          this.passwordValidations.uppercase &&\n          this.passwordValidations.special &&\n          this.passwordValidations.noUsername &&\n          this.passwordValidations.noOldPassword\n      );\n    },\n    hasChanges() {\n      return (\n          this.name !== this.originalData.name ||\n          this.surname !== this.originalData.surname ||\n          this.dob !== this.originalData.dob ||\n          this.email !== this.originalData.email ||\n          this.phone !== this.originalData.phone ||\n          this.favParty !== this.originalData.favParty ||\n          this.oldPassword ||\n          this.newPassword\n      );\n    }\n  },\n  methods: {\n    applyChanges() {\n      if (this.isPasswordValid && !this.oldPassword) {\n        console.log(\"Changes applied\");\n        this.originalData = {\n          name: this.name,\n          surname: this.surname,\n          dob: this.dob,\n          email: this.email,\n          phone: this.phone,\n          favParty: this.favParty\n        };\n      } else {\n        console.log(\"Password does not meet the requirements\");\n      }\n    },\n    deleteAccount() {\n      console.log(\"Account deleted\");\n      this.showDeleteModal = false;\n    },\n    onFileChange(e) {\n      const file = e.target.files[0];\n      this.profilePicture = URL.createObjectURL(file);\n    }\n  },\n  mounted() {\n    this.originalData = {\n      name: this.name,\n      surname: this.surname,\n      dob: this.dob,\n      email: this.email,\n      phone: this.phone,\n      favParty: this.favParty\n    };\n  }\n};\n</script>\n\n<style scoped>\n* {\n  font-family: \"Outfit\", sans-serif;\n}\n\n.profile-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background-color: #f3f3f3;\n  padding: 20px;\n}\n\n.profile-form {\n  width: 450px;\n  padding: 30px;\n  border: 2px solid #1a123b;\n  border-radius: 15px;\n  background-color: #fff;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\nh1 {\n  color: #1a123b;\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.form-row {\n  display: flex;\n  justify-content: space-between;\n  gap: 20px;\n  margin-bottom: 20px;\n}\n\n.form-group {\n  display: flex;\n  flex-direction: column;\n  margin-bottom: 20px;\n  width: 100%;\n}\n\n.full-width {\n  width: 100%;\n}\n\n.half-width {\n  width: 48%;\n}\n\nlabel {\n  font-weight: bold;\n  margin-bottom: 5px;\n}\n\ninput, select {\n  padding: 10px;\n  border: 2px solid #1a123b;\n  border-radius: 10px;\n  font-size: 16px;\n  color: #333;\n}\n\nselect {\n  height: auto;\n}\n\n.profile-picture-placeholder {\n  width: 80px;\n  height: 80px;\n  border: 2px solid #1a123b;\n  border-radius: 50%;\n  margin: 0 0 20px 0;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  overflow: hidden;\n}\n\n.profile-picture {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.form-buttons {\n  display: flex;\n  justify-content: space-between;\n}\n\n.delete-btn {\n  padding: 10px;\n  background-color: #ff4d4d;\n  color: white;\n  border: none;\n  border-radius: 20px;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.apply-btn {\n  padding: 10px;\n  background-color: #ff9933;\n  color: white;\n  border: none;\n  border-radius: 20px;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.delete-btn:hover {\n  background-color: #e60000;\n}\n\n.apply-btn:hover {\n  background-color: #e68a00;\n}\n\n.password-requirements {\n  list-style-type: none;\n  padding: 0;\n  margin-top: 10px;\n  font-size: 12px;\n}\n\n.password-requirements li {\n  margin-bottom: 5px;\n}\n\n.password-requirements .valid {\n  color: green;\n}\n\n.password-requirements .invalid {\n  color: red;\n}\n\n.modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n\n.modal-content {\n  background-color: white;\n  padding: 30px;\n  border-radius: 10px;\n  text-align: center;\n  width: 300px;\n}\n\n.modal-buttons {\n  margin-top: 20px;\n  display: flex;\n  justify-content: space-between;\n}\n\n.confirm-btn, .cancel-btn {\n  padding: 10px;\n  width: 100px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.confirm-btn {\n  background-color: #ff4d4d;\n  color: white;\n}\n\n.confirm-btn:hover {\n  background-color: #e60000;\n}\n\n.cancel-btn {\n  background-color: #cccccc;\n}\n\n.cancel-btn:hover {\n  background-color: #999999;\n}\n</style>\n"],"mappings":";;;AAyIA,eAAe;EACbA,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,OAAO,EAAE,EAAE;MACXC,GAAG,EAAE,EAAE;MACPC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,IAAI;MACpBC,kBAAkB,EAAE,KAAK;MACzBC,YAAY,EAAE,CAAC,CAAC;MAChBC,eAAe,EAAE,KAAK;MACtBC,OAAO,EAAE,CACP,KAAK,EACL,KAAK,EACL,0CAA0C,EAC1C,+BAA+B,EAC/B,KAAK,EACL,4BAA4B,EAC5B,uBAAuB,EACvB,uBAAuB,EACvB,cAAc,EACd,MAAM,EACN,MAAM,EACN,wCAAwC,EACxC,MAAM,EACN,QAAQ,EACR,KAAK,EACL,MAAM,EACN,4BAA4B,EAC5B,uBAAuB,EACvB,wBAAwB,EACxB,UAAU,EACV,wBAAwB,EACxB,gCAAgC,EAChC,sBAAsB,EACtB,wBAAwB,EACxB,eAAe,EACf,oCAAmC;IAEvC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,mBAAmBA,CAAA,EAAG;MACpB,MAAMC,QAAO,GAAI,IAAI,CAACR,WAAW;MACjC,MAAMN,QAAO,GAAI,IAAI,CAACA,QAAQ;MAC9B,MAAMK,WAAU,GAAI,IAAI,CAACA,WAAW;MACpC,OAAO;QACLU