else {\n        this.errorMessage = \"Failed to load profile information.\";\n        if (response.status === 401) {\n          this.$router.push(\"/login\");\n        }\n      }\n    } catch (error) {\n      console.error(\"Error loading profile:\", error);\n      this.errorMessage = \"An error occurred while loading your profile.\";\n    } finally {\n      this.loading = false;\n    }\n  },\n  methods: {\n    async updateProfile() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const response = await fetch(`${API_URL}/api/v1/User/update/${this.user.username}`, {\n          method: \"PUT\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify(this.user)\n        });\n\n        if (response.ok) {\n          this.successMessage = \"Profile updated successfully!\";\n          this.editMode = false;\n          this.originalUser = JSON.parse(JSON.stringify(this.user)); // Update originele gegevens\n        } else {\n          this.errorMessage = \"Failed to update profile.\";\n        }\n      } catch (error) {\n        console.error(\"Error updating profile:\", error);\n        this.errorMessage = \"An error occurred while updating your profile.\";\n      }\n    },\n    cancelEdit() {\n      this.user = JSON.parse(JSON.stringify(this.originalUser)); // Herstel originele gegevens\n      this.editMode = false;\n      this.successMessage = \"\";\n      this.errorMessage = \"\";\n    }\n  }\n};\n</script>\n\n\n<style scoped>\n.profile-actions,\n.form-actions {\n  margin-top: 20px;\n}\nbutton {\n  margin-right: 10px;\n}\n.success {\n  color: green;\n}\n.error {\n  color: red;\n}\n</style>\n\n"],"mappings":";AA6CA,SAASA,OAAM,QAAS,UAAU;AAElC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,GAAG,EAAE,EAAE;QACPC,MAAM,EAAE;MACV,CAAC;MACDC,YAAY,EAAE,CAAC,CAAC;MA