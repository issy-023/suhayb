{"ast":null,"code":"export default {\n  name: \"PartyDetailComponent\",\n  props: [\"partyId\"],\n  data() {\n    return {\n      party: {},\n      votes: {\n        totalVotes: 0,\n        seats: 0,\n        remainingVotes: 0\n      },\n      candidates: [],\n      showAll: false\n    };\n  },\n  computed: {\n    visibleCandidates() {\n      return this.showAll ? this.candidates : this.candidates.slice(0, 10);\n    },\n    showMoreButton() {\n      return !this.showAll && this.candidates.length > 10;\n    }\n  },\n  async mounted() {\n    if (!this.partyId) {\n      console.error(\"Party ID is undefined.\");\n      return;\n    }\n    await this.fetchPartyDetails(); // Zorg ervoor dat party details eerst worden opgehaald\n    this.fetchCandidates(); // Haal kandidaten op nadat partyId beschikbaar is\n    this.calculateVotes();\n  },\n  methods: {\n    async fetchPartyDetails() {\n      try {\n        const response = await fetch(`http://localhost:8084/api/v1/parties/${this.partyId}`);\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        const data = await response.json();\n        this.party = data;\n      } catch (error) {\n        console.error(\"Error fetching party details:\", error);\n      }\n    },\n    async calculateVotes() {\n      try {\n        const response = await fetch(\"http://localhost:8084/api/election-results/votes-per-party\");\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        const data = await response.json();\n        const partyVotes = data.find(party => party.name === this.party.name);\n        if (partyVotes) {\n          const totalSeats = 150;\n          const kiesdeler = Math.floor(data.reduce((sum, party) => sum + party.totalVotes, 0) / totalSeats);\n          const seats = Math.floor(partyVotes.totalVotes / kiesdeler);\n          const remainingVotes = kiesdeler - partyVotes.totalVotes % kiesdeler;\n          this.votes = {\n            totalVotes: partyVotes.totalVotes,\n            seats,\n            remainingVotes: remainingVotes > 0 ? remainingVotes : 0\n          };\n        }\n      } catch (error) {\n        console.error(\"Error fetching votes data:\", error);\n      }\n    },\n    async fetchCandidates() {\n      try {\n        const response = await fetch(`http://localhost:8084/api/v1/candidates/by-party/${this.partyId}`);\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        this.candidates = (await response.json()).sort((a, b) => b.totalVotes - a.totalVotes);\n      } catch (error) {\n        console.error(\"Error fetching candidates:\", error);\n      }\n    },\n    sortCandidates(order) {\n      if (order === \"votes-asc\") {\n        this.candidates.sort((a, b) => a.totalVotes - b.totalVotes);\n      } else if (order === \"votes-desc\") {\n        this.candidates.sort((a, b) => b.totalVotes - a.totalVotes);\n      }\n    },\n    showAllCandidates() {\n      this.showAll = true;\n    },\n    formatVotes(votes) {\n      return votes.toLocaleString();\n    }\n  }\n};","map":{"version":3,"names":["name","props","data","party","votes","totalVotes","seats","remainingVotes","candidates","showAll","computed","visibleCandidates","slice","showMoreButton","length","mounted","partyId","console","error","fetchPartyDetails","fetchCandidates","calculateVotes","methods","response","fetch","ok","Error","status","json","partyVotes","find","totalSeats","kiesdeler","Math","floor","reduce","sum","sort","a","b","sortCandidates","order","showAllCandidates","formatVotes","toLocaleString"],"sources":["/Users/suhayb/MijnProjecten/cuuruuvoovii16/election-frontend/src/components/PartyDetailComponent.vue"],"sourcesContent":["<template>\n  <div class=\"party-detail\">\n    <h1>{{ party.name }}</h1>\n\n    <div class=\"vote-details\">\n      <h2>Votes and Seats</h2>\n      <p><strong>Total Votes:</strong> {{ formatVotes(votes.totalVotes) }}</p>\n      <p><strong>Seats:</strong> {{ votes.seats }}</p>\n      <p v-if=\"votes.remainingVotes > 0\">\n        <strong>Votes Needed for Next Seat:</strong> {{ formatVotes(votes.remainingVotes) }}\n      </p>\n    </div>\n\n    <div class=\"candidates\">\n      <h2>Candidates</h2>\n      <div class=\"filter-buttons\">\n        <button @click=\"sortCandidates('votes-desc')\">Votes: High to Low</button>\n        <button @click=\"sortCandidates('votes-asc')\">Votes: Low to High</button>\n      </div>\n      <ul>\n        <li\n            v-for=\"(candidate) in visibleCandidates\"\n            :key=\"candidate.id\"\n        >\n          <div class=\"candidate-item\">\n            <span class=\"candidate-name\">{{ candidate.name }}</span>\n            <span class=\"candidate-votes\">{{ formatVotes(candidate.totalVotes) }} votes</span>\n          </div>\n        </li>\n      </ul>\n      <button v-if=\"showMoreButton\" @click=\"showAllCandidates\" class=\"load-more-btn\">\n        More Candidates\n      </button>\n    </div>\n\n    <router-link to=\"/parties\" class=\"back-link\">Back to Parties</router-link>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"PartyDetailComponent\",\n  props: [\"partyId\"],\n  data() {\n    return {\n      party: {},\n      votes: {\n        totalVotes: 0,\n        seats: 0,\n        remainingVotes: 0,\n      },\n      candidates: [],\n      showAll: false,\n    };\n  },\n  computed: {\n    visibleCandidates() {\n      return this.showAll ? this.candidates : this.candidates.slice(0, 10);\n    },\n    showMoreButton() {\n      return !this.showAll && this.candidates.length > 10;\n    },\n  },\n  async mounted() {\n    if (!this.partyId) {\n      console.error(\"Party ID is undefined.\");\n      return;\n    }\n    await this.fetchPartyDetails(); // Zorg ervoor dat party details eerst worden opgehaald\n    this.fetchCandidates(); // Haal kandidaten op nadat partyId beschikbaar is\n    this.calculateVotes();\n  },\n  methods: {\n    async fetchPartyDetails() {\n      try {\n        const response = await fetch(`http://localhost:8084/api/v1/parties/${this.partyId}`);\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        const data = await response.json();\n        this.party = data;\n      } catch (error) {\n        console.error(\"Error fetching party details:\", error);\n      }\n    },\n    async calculateVotes() {\n      try {\n        const response = await fetch(\"http://localhost:8084/api/election-results/votes-per-party\");\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        const data = await response.json();\n        const partyVotes = data.find((party) => party.name === this.party.name);\n\n        if (partyVotes) {\n          const totalSeats = 150;\n          const kiesdeler = Math.floor(\n              data.reduce((sum, party) => sum + party.totalVotes, 0) / totalSeats\n          );\n          const seats = Math.floor(partyVotes.totalVotes / kiesdeler);\n          const remainingVotes = kiesdeler - (partyVotes.totalVotes % kiesdeler);\n\n          this.votes = {\n            totalVotes: partyVotes.totalVotes,\n            seats,\n            remainingVotes: remainingVotes > 0 ? remainingVotes : 0,\n          };\n        }\n      } catch (error) {\n        console.error(\"Error fetching votes data:\", error);\n      }\n    },\n    async fetchCandidates() {\n      try {\n        const response = await fetch(\n            `http://localhost:8084/api/v1/candidates/by-party/${this.partyId}`\n        );\n        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n        this.candidates = (await response.json()).sort(\n            (a, b) => b.totalVotes - a.totalVotes\n        );\n      } catch (error) {\n        console.error(\"Error fetching candidates:\", error);\n      }\n    },\n    sortCandidates(order) {\n      if (order === \"votes-asc\") {\n        this.candidates.sort((a, b) => a.totalVotes - b.totalVotes);\n      } else if (order === \"votes-desc\") {\n        this.candidates.sort((a, b) => b.totalVotes - a.totalVotes);\n      }\n    },\n    showAllCandidates() {\n      this.showAll = true;\n    },\n    formatVotes(votes) {\n      return votes.toLocaleString();\n    },\n  },\n};\n</script>\n\n<style scoped>\n.party-detail {\n  text-align: center;\n  padding: 20px;\n  background-color: #f5f5f5;\n  border-radius: 10px;\n  max-width: 800px;\n  margin: 20px auto;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nh1 {\n  font-size: 2.5rem;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.vote-details {\n  margin-top: 20px;\n  text-align: left;\n  display: inline-block;\n  background-color: #ffffff;\n  border: 1px solid #ddd;\n  border-radius: 10px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  width: 100%;\n}\n\n.vote-details h2 {\n  font-size: 1.5rem;\n  margin-bottom: 10px;\n  color: #444;\n}\n\n.vote-details p {\n  margin: 5px 0;\n  font-size: 1rem;\n  color: #666;\n}\n\n.candidates {\n  margin-top: 20px;\n  text-align: left;\n  background-color: #ffffff;\n  border: 1px solid #ddd;\n  border-radius: 10px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.candidates h2 {\n  font-size: 1.5rem;\n  margin-bottom: 10px;\n  color: #444;\n}\n\n.filter-buttons {\n  margin-bottom: 20px;\n  text-align: center;\n}\n\n.filter-buttons button {\n  margin: 0 10px;\n  padding: 10px 20px;\n  background-color: #ec7700;\n  border: none;\n  border-radius: 5px;\n  color: white;\n  cursor: pointer;\n  font-size: 1rem;\n}\n\n.filter-buttons button:hover {\n  background-color: #c26200;\n}\n\n.candidates ul {\n  list-style-type: none;\n  padding: 0;\n}\n\n.candidate-item {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px 0;\n  border-bottom: 1px solid #ddd;\n}\n\n.candidate-name {\n  font-size: 1rem;\n  color: #333;\n}\n\n.candidate-votes {\n  font-size: 1rem;\n  color: #555;\n}\n\n.load-more-btn {\n  margin-top: 20px;\n  padding: 10px 20px;\n  background-color: #ec7700;\n  border: none;\n  border-radius: 5px;\n  color: white;\n  cursor: pointer;\n  font-size: 1rem;\n}\n\n.load-more-btn:hover {\n  background-color: #c26200;\n}\n\n.back-link {\n  display: inline-block;\n  margin-top: 20px;\n  text-decoration: none;\n  color: #ec7700;\n  font-weight: bold;\n}\n\n.back-link:hover {\n  text-decoration: underline;\n}\n</style>\n"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,sBAAsB;EAC5BC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,KAAK,EAAE;QACLC,UAAU,EAAE,CAAC;QACbC,KAAK,EAAE,CAAC;QACRC,cAAc,EAAE;MAClB,CAAC;MACDC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACF,OAAM,GAAI,IAAI,CAACD,UAAS,GAAI,IAAI,CAACA,UAAU,CAACI,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;IACtE,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,OAAO,CAAC,IAAI,CAACJ,OAAM,IAAK,IAAI,CAACD,UAAU,CAACM,MAAK,GAAI,EAAE;IACrD;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAAC,IAAI,CAACC,OAAO,EAAE;MACjBC,OAAO,CAACC,KAAK,CAAC,wBAAwB,CAAC;MACvC;IACF;IACA,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC,EAAE;IAChC,IAAI,CAACC,eAAe,CAAC,CAAC,EAAE;IACxB,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMH,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMC,KAAK,CAAC,wCAAwC,IAAI,CAACR,OAAO,EAAE,CAAC;QACpF,IAAI,CAACO,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,uBAAuBH,QAAQ,CAACI,MAAM,EAAE,CAAC;QAC3E,MAAMzB,IAAG,GAAI,MAAMqB,QAAQ,CAACK,IAAI,CAAC,CAAC;QAClC,IAAI,CAACzB,KAAI,GAAID,IAAI;MACnB,EAAE,OAAOgB,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACD,MAAMG,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,4DAA4D,CAAC;QAC1F,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,uBAAuBH,QAAQ,CAACI,MAAM,EAAE,CAAC;QAC3E,MAAMzB,IAAG,GAAI,MAAMqB,QAAQ,CAACK,IAAI,CAAC,CAAC;QAClC,MAAMC,UAAS,GAAI3B,IAAI,CAAC4B,IAAI,CAAE3B,KAAK,IAAKA,KAAK,CAACH,IAAG,KAAM,IAAI,CAACG,KAAK,CAACH,IAAI,CAAC;QAEvE,IAAI6B,UAAU,EAAE;UACd,MAAME,UAAS,GAAI,GAAG;UACtB,MAAMC,SAAQ