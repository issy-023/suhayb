{"ast":null,"code":"import { Chart, registerables } from 'chart.js';\nimport { nextTick } from 'vue';\nChart.register(...registerables);\nexport default {\n  name: 'DataPage',\n  data() {\n    return {\n      currentView: 'currentResults',\n      chartInstance: null,\n      candidateResults: [],\n      // Lijst van kandidaten (vanuit de API)\n      parties: [],\n      // Lijst van partijen (vanuit de API)\n      districts: [],\n      // Districten lijst (vanuit de API)\n      selectedDistrict: null\n    };\n  },\n  mounted() {\n    this.fetchCandidateResults();\n    this.fetchPartyResults();\n    this.fetchConstituencies();\n    this.renderChart();\n  },\n  watch: {\n    currentView() {\n      this.renderChart();\n    }\n  },\n  methods: {\n    // Haal kandidaten op vanuit de API\n    async fetchCandidateResults() {\n      try {\n        const response = await fetch('/api/candidates');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.candidateResults = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van kandidaten', error);\n      }\n    },\n    // Haal de partij resultaten (zetels per partij) op\n    async fetchPartyResults() {\n      try {\n        const response = await fetch('/api/parties');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.parties = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van partijen', error);\n      }\n    },\n    // Haal kiesdistricten op vanuit de API\n    async fetchConstituencies() {\n      try {\n        const response = await fetch('/api/constituencies');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.districts = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van districten', error);\n      }\n    },\n    renderChart() {\n      nextTick(() => {\n        if (this.currentView === 'currentResults') {\n          const ctx = this.$refs.currentResultsChart?.getContext('2d');\n          if (this.chartInstance) {\n            this.chartInstance.destroy();\n          }\n          if (ctx) {\n            // Hier gebruik je de opgehaalde partijdata voor de grafiek\n            const partyNames = this.parties.map(party => party.name);\n            const seatCounts = this.parties.map(party => party.seats);\n            this.chartInstance = new Chart(ctx, {\n              type: 'bar',\n              data: {\n                labels: partyNames,\n                datasets: [{\n                  label: 'Aantal Zetels',\n                  data: seatCounts,\n                  backgroundColor: ['#003f7f', '#00a85a', '#ffcc00', '#8a2be2', '#f1c232', '#228b22', '#0066ff', '#e30513', '#800000', '#ff69b4', '#4b5320', '#b8860b', '#1c1c1c', '#660066', '#cc0000'],\n                  borderColor: '#ffffff',\n                  borderWidth: 2\n                }]\n              },\n              options: {\n                plugins: {\n                  legend: {\n                    labels: {\n                      color: '#ffffff'\n                    }\n                  }\n                },\n                scales: {\n                  x: {\n                    ticks: {\n                      color: '#ffffff'\n                    }\n                  },\n                  y: {\n                    ticks: {\n                      color: '#ffffff'\n                    }\n                  }\n                }\n              }\n            });\n          }\n        }\n      });\n    },\n    renderConstituencyChart() {\n      nextTick(() => {\n        const ctx = this.$refs.constituencyResultsChart?.getContext('2d');\n        if (this.chartInstance) {\n          this.chartInstance.destroy();\n        }\n        if (ctx && this.selectedDistrict) {\n          const districtResults = this.selectedDistrict;\n          this.chartInstance = new Chart(ctx, {\n            type: 'bar',\n            data: {\n              labels: ['PVV', 'GroenLinks-PvdA', 'VVD', 'Nieuw Sociaal Contract', 'D66'],\n              datasets: [{\n                label: 'Aantal Zetels',\n                data: [districtResults.pvv, districtResults.groenlinks_pvda, districtResults.vvd, districtResults.nieuw_sociaal_contract, districtResults.d66],\n                backgroundColor: ['#003f7f', '#00a85a', '#ffcc00', '#8a2be2', '#f1c232'],\n                borderColor: '#ffffff',\n                borderWidth: 2\n              }]\n            },\n            options: {\n              plugins: {\n                legend: {\n                  labels: {\n                    color: '#ffffff'\n                  }\n                }\n              },\n              scales: {\n                x: {\n                  ticks: {\n                    color: '#ffffff'\n                  }\n                },\n                y: {\n                  ticks: {\n                    color: '#ffffff'\n                  }\n                }\n              }\n            }\n          });\n        }\n      });\n    },\n    toggleDetails(index) {\n      this.candidateResults[index].showDetails = !this.candidateResults[index].showDetails;\n    },\n    showCurrentResults() {\n      this.currentView = 'currentResults';\n    },\n    showCandidateResults() {\n      this.currentView = 'candidateResults';\n    },\n    showConstituencyResults() {\n      this.currentView = 'constituencyResults';\n    }\n  }\n};","map":{"version":3,"names":["Chart","registerables","nextTick","register","name","data","currentView","chartInstance","candidateResults","parties","districts","selectedDistrict","mounted","fetchCandidateResults","fetchPartyResults","fetchConstituencies","renderChart","watch","methods","response","fetch","ok","Error","json","error","console","ctx","$refs","currentResultsChart","getContext","destroy","partyNames","map","party","seatCounts","seats","type","labels","datasets","label","backgroundColor","borderColor","borderWidth","options","plugins","legend","color","scales","x","ticks","y","renderConstituencyChart","constituencyResultsChart","districtResults","pvv","groenlinks_pvda","vvd","nieuw_sociaal_contract","d66","toggleDetails","index","showDetails","showCurrentResults","showCandidateResults","showConstituencyResults"],"sources":["/Users/suhayb/Documenten/cuuruuvoovii16/election-frontend/src/components/DataPageComponent.vue"],"sourcesContent":["<template>\n  <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n  <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n  <link href=\"https://fonts.googleapis.com/css2?family=Outfit:wght@100..900&family=Roboto:wght@400;700&display=swap\"\n        rel=\"stylesheet\">\n  <div>\n    <h1>Election Results</h1>\n\n    <div class=\"button-container\">\n      <button @click=\"showCurrentResults\" class=\"switch-button\">Huidige Verkiezingsuitslagen</button>\n      <button @click=\"showCandidateResults\" class=\"switch-button\">Zetels per Kandidaat</button>\n      <button @click=\"showConstituencyResults\" class=\"switch-button\">Uitslagen per Kiesdistrict</button>\n    </div>\n\n    <div v-if=\"currentView === 'candidateResults'\" class=\"winner-list\">\n      <h2>Zetels per Kandidaat</h2>\n      <ul class=\"winner-list-container\">\n        <li v-for=\"(candidate, index) in candidateResults\" :key=\"candidate.naam\" class=\"winner-item\">\n          <div @click=\"toggleDetails(index)\" class=\"winner-details\">\n            <strong>{{ candidate.naam }}</strong>\n            <span>{{ candidate.partij }} - {{ candidate.zetels }} zetels</span>\n          </div>\n\n          <div v-if=\"candidate.showDetails\" class=\"extra-info\">\n            <p>{{ candidate.extraInfo }}</p>\n          </div>\n        </li>\n      </ul>\n    </div>\n\n    <div v-else-if=\"currentView === 'currentResults'\" class=\"chart-container\">\n      <h2>Huidige Verkiezingsuitslagen - Zetels per Partij</h2>\n      <canvas ref=\"currentResultsChart\"></canvas>\n    </div>\n\n    <div v-else-if=\"currentView === 'constituencyResults'\" class=\"chart-container\">\n      <h2>Uitslagen per Kiesdistrict</h2>\n\n      <!-- Dropdown menu for selecting a district -->\n      <div class=\"dropdown-container\">\n        <label for=\"districtSelect\">Selecteer een kiesdistrict:</label>\n        <select v-model=\"selectedDistrict\" @change=\"renderConstituencyChart\">\n          <option v-for=\"(district, index) in districts\" :key=\"index\" :value=\"district\">\n            {{ district.name }}\n          </option>\n        </select>\n      </div>\n\n      <!-- Chart is displayed only after a district is selected -->\n      <canvas v-if=\"selectedDistrict\" ref=\"constituencyResultsChart\"></canvas>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Chart, registerables } from 'chart.js';\nimport { nextTick } from 'vue';\n\nChart.register(...registerables);\n\nexport default {\n  name: 'DataPage',\n  data() {\n    return {\n      currentView: 'currentResults',\n      chartInstance: null,\n      candidateResults: [],  // Lijst van kandidaten (vanuit de API)\n      parties: [],  // Lijst van partijen (vanuit de API)\n      districts: [],  // Districten lijst (vanuit de API)\n      selectedDistrict: null,\n    };\n  },\n  mounted() {\n    this.fetchCandidateResults();\n    this.fetchPartyResults();\n    this.fetchConstituencies();\n    this.renderChart();\n  },\n  watch: {\n    currentView() {\n      this.renderChart();\n    }\n  },\n  methods: {\n    // Haal kandidaten op vanuit de API\n    async fetchCandidateResults() {\n      try {\n        const response = await fetch('/api/candidates');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.candidateResults = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van kandidaten', error);\n      }\n    },\n    // Haal de partij resultaten (zetels per partij) op\n    async fetchPartyResults() {\n      try {\n        const response = await fetch('/api/parties');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.parties = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van partijen', error);\n      }\n    },\n    // Haal kiesdistricten op vanuit de API\n    async fetchConstituencies() {\n      try {\n        const response = await fetch('/api/constituencies');\n        if (!response.ok) {\n          throw new Error('Netwerk antwoord was niet ok');\n        }\n        const data = await response.json();\n        this.districts = data;\n      } catch (error) {\n        console.error('Fout bij het ophalen van districten', error);\n      }\n    },\n    renderChart() {\n      nextTick(() => {\n        if (this.currentView === 'currentResults') {\n          const ctx = this.$refs.currentResultsChart?.getContext('2d');\n          if (this.chartInstance) {\n            this.chartInstance.destroy();\n          }\n          if (ctx) {\n            // Hier gebruik je de opgehaalde partijdata voor de grafiek\n            const partyNames = this.parties.map(party => party.name);\n            const seatCounts = this.parties.map(party => party.seats);\n            this.chartInstance = new Chart(ctx, {\n              type: 'bar',\n              data: {\n                labels: partyNames,\n                datasets: [{\n                  label: 'Aantal Zetels',\n                  data: seatCounts,\n                  backgroundColor: [\n                    '#003f7f', '#00a85a', '#ffcc00', '#8a2be2', '#f1c232',\n                    '#228b22', '#0066ff', '#e30513', '#800000', '#ff69b4',\n                    '#4b5320', '#b8860b', '#1c1c1c', '#660066', '#cc0000'\n                  ],\n                  borderColor: '#ffffff',\n                  borderWidth: 2\n                }]\n              },\n              options: {\n                plugins: {\n                  legend: {\n                    labels: {\n                      color: '#ffffff'\n                    }\n                  }\n                },\n                scales: {\n                  x: {\n                    ticks: {\n                      color: '#ffffff'\n                    }\n                  },\n                  y: {\n                    ticks: {\n                      color: '#ffffff'\n                    }\n                  }\n                }\n              }\n            });\n          }\n        }\n      });\n    },\n    renderConstituencyChart() {\n      nextTick(() => {\n        const ctx = this.$refs.constituencyResultsChart?.getContext('2d');\n        if (this.chartInstance) {\n          this.chartInstance.destroy();\n        }\n        if (ctx && this.selectedDistrict) {\n          const districtResults = this.selectedDistrict;\n          this.chartInstance = new Chart(ctx, {\n            type: 'bar',\n            data: {\n              labels: ['PVV', 'GroenLinks-PvdA', 'VVD', 'Nieuw Sociaal Contract', 'D66'],\n              datasets: [\n                {\n                  label: 'Aantal Zetels',\n                  data: [districtResults.pvv, districtResults.groenlinks_pvda, districtResults.vvd, districtResults.nieuw_sociaal_contract, districtResults.d66],\n                  backgroundColor: ['#003f7f', '#00a85a', '#ffcc00', '#8a2be2', '#f1c232'],\n                  borderColor: '#ffffff',\n                  borderWidth: 2\n                }\n              ]\n            },\n            options: {\n              plugins: {\n                legend: {\n                  labels: {\n                    color: '#ffffff'\n                  }\n                }\n              },\n              scales: {\n                x: {\n                  ticks: {\n                    color: '#ffffff'\n                  }\n                },\n                y: {\n                  ticks: {\n                    color: '#ffffff'\n                  }\n                }\n              }\n            }\n          });\n        }\n      });\n    },\n    toggleDetails(index) {\n      this.candidateResults[index].showDetails = !this.candidateResults[index].showDetails;\n    },\n    showCurrentResults() {\n      this.currentView = 'currentResults';\n    },\n    showCandidateResults() {\n      this.currentView = 'candidateResults';\n    },\n    showConstituencyResults() {\n      this.currentView = 'constituencyResults';\n    }\n  }\n}\n</script>\n\n\n\n<style scoped>\n* {\n  font-family: \"Outfit\", sans-serif;\n  font-optical-sizing: auto;\n  font-style: normal;\n}\n\n.chart-container, .winner-list {\n  width: 90%;\n  max-width: 930px;\n  background-color: #17003C;\n  padding: 20px;\n  border-radius: 15px;\n  border: 5px solid #FF9900;\n  margin: 30px auto 120px auto;\n}\n\nh1 {\n  text-align: center;\n  margin: 150px 0 40px 0;\n  color: #17003C;\n}\n\n.button-container {\n  display: flex;\n  justify-content: center;\n  gap: 10px;\n  margin-top: 50px;\n}\n\nh2 {\n  text-align: center;\n  margin-bottom: 20px;\n  color: #ffffff;\n}\n\n.winner-list-container {\n  padding: 0;\n}\n\n.winner-item {\n  background-color: transparent;\n  border: 3px solid #ffffff;\n  padding: 15px;\n  margin: 10px 0;\n  border-radius: 8px;\n  transition: background-color 0.3s ease;\n  cursor: pointer;\n  color: #ffffff;\n}\n\n.winner-item:hover {\n  background-color: #FF9900;\n  border: 3px solid #ffffff;\n}\n\n.winner-details {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n  color: #ffffff;\n}\n\n.extra-info {\n  padding: 10px 0;\n  color: #ffffff;\n  font-style: italic;\n}\n\n.switch-button {\n  padding: 20px 40px;\n  background-color: #17003C;\n  color: white;\n  border: 5px solid #FF9900;\n  border-radius: 50px;\n  cursor: pointer;\n  font-size: 1.2em;\n  transition: background-color 0.3s ease, transform 0.2s ease;\n}\n\n.switch-button:hover {\n  background-color: #FF9900;\n  border-color: #17003C;\n  transform: scale(1.05);\n}\n\n.dropdown-container {\n  margin: 20px auto;\n  text-align: center;\n  color:white;\n}\n\nselect {\n  padding: 10px;\n  font-size: 1.2em;\n  border-radius: 5px;\n}\n</style>\n"],"mappings":"AAuDA,SAASA,KAAK,EAAEC,aAAY,QAAS,UAAU;AAC/C,SAASC,QAAO,QAAS,KAAK;AAE9BF,KAAK,CAACG,QAAQ,CAAC,GAAGF,aAAa,CAAC;AAEhC,eAAe;EACbG,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,gBAAgB;MAC7BC,aAAa,EAAE,IAAI;MACnBC,gBAAgB,EAAE,EAAE;MAAG;MACvBC,OAAO,EAAE,EAAE;MAAG;MACdC,SAAS,EAAE,EAAE;MAAG;MAChBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,qBAAqB,CAAC,CAAC;IAC5B,IAAI,CAACC,iBAAiB,CAAC,CAAC;IACxB,IAAI,CAACC,mBAAmB,CAAC,CAAC;IAC1B,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,KAAK,EAAE;IACLX,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACU,WAAW,CAAC,CAAC;IACpB;EACF,CAAC;EACDE,OAAO,EAAE;IACP;IACA,MAAML,qBAAqBA,CAAA,EAAG;MAC5B,IAAI;QACF,MAAMM,QAAO,GAAI,MAAMC,KAAK,CAAC,iBAAiB,CAAC;QAC/C,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,8BAA8B,CAAC;QACjD;QACA,MAAMjB,IAAG,GAAI,MAAMc,QAAQ,CAACI,IAAI,CAAC,CAAC;QAClC,IAAI,CAACf,gBAAe,GAAIH,IAAI;MAC9B,EAAE,OAAOmB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,qCAAqC,EAAEA,KAAK,CAAC;MAC7D;IACF,CAAC;IACD;IACA,MAAMV,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAMK,QAAO,GAAI,MAAMC,KAAK,CAAC,cAAc,CAAC;QAC5C,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,8BAA8B,CAAC;QACjD;QACA,MAAMjB,IAAG,GAAI,MAAMc,QAAQ,CAACI,IAAI,CAAC,CAAC;QAClC,IAAI,CAACd,OAAM,GAAIJ,IAAI;MACrB,EAAE,OAAOmB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mCAAmC,EAAEA,KAAK,CAAC;MAC3D;IACF,CAAC;IACD;IACA,MAAMT,mBAAmBA,CAAA,EAAG;MAC1B,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMC,KAAK,CAAC,qBAAqB,CAAC;QACnD,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,8BAA8B,CAAC;QACjD;QACA,MAAMjB,IAAG,GAAI,MAAMc,Q