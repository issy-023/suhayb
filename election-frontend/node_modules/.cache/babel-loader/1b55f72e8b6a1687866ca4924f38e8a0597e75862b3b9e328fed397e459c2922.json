{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PartyAdapter from '../adapters/PartyAdapter.js';\nexport default {\n  name: \"AllPartiesComponent\",\n  data() {\n    return {\n      title: 'title',\n      currentPage: 0,\n      partiesPerPage: 9,\n      parties: [],\n      totalPages: 0,\n      url: \"http://localhost:8084\"\n    };\n  },\n  methods: {\n    redirectToPartyDetail(party) {\n      this.$router.push({\n        name: 'PartyDetail',\n        params: {\n          partyId: party.id\n        },\n        query: {\n          description: party.description,\n          importantPoints: JSON.stringify(party.importantPoints)\n        }\n      });\n    },\n    async goToPage(page) {\n      if (page >= 0 && page < this.totalPages) {\n        this.currentPage = page;\n        await this.loadParties();\n      }\n    },\n    async nextPage() {\n      if (this.currentPage + 1 < this.totalPages) {\n        this.currentPage++;\n        await this.loadParties();\n      }\n    },\n    async previousPage() {\n      if (this.currentPage > 0) {\n        this.currentPage--;\n        await this.loadParties();\n      }\n    },\n    async loadParties() {\n      const adapter = new PartyAdapter(this.url);\n      const response = await adapter.getParties(this.currentPage, this.partiesPerPage);\n      this.parties = response.parties;\n      this.totalPages = response.totalPages;\n    }\n  },\n  async mounted() {\n    await this.loadParties();\n  }\n};","map":{"version":3,"names":["PartyAdapter","name","data","title","currentPage","partiesPerPage","parties","totalPages","url","methods","redirectToPartyDetail","party","$router","push","params","partyId","id","query","description","importantPoints","JSON","stringify","goToPage","page","loadParties","nextPage","previousPage","adapter","response","getParties","mounted"],"sources":["/Users/suhayb/MijnProjecten/cuuruuvoovii16/election-frontend/src/components/EveryPartyComponent.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1 :class=\"title\">Choose Your Political Party for More Information</h1>\n    <div class=\"intro\">\n      <h2 class=\"text\">Welcome! On this page, you can easily learn more about the\n        different political parties in our country. Whether you're\n        interested in their policies, vision, or election programs,\n        we've gathered everything for you.</h2>\n      <h2 class=\"text\">\n        Already have a party in mind? Discover everything about\n        their policies and plans by simply clicking on the party\n        of your choice!\n      </h2>\n    </div>\n    <div class=\"party-container\">\n      <div v-for=\"party in parties\" :key=\"party.id\" class=\"party-box\">\n        <img class=\"image\" :src=\"party.image\" :alt=\"party.name\"/>\n        <div class=\"button\" @click=\"redirectToPartyDetail(party)\">More Info</div>\n      </div>\n    </div>\n    <div class=\"navigation\">\n      <div @click=\"previousPage\" class=\"button\" :class=\"{ disabled: currentPage === 0 }\">\n        Previous page\n      </div>\n      <div v-for=\"page in totalPages\" :key=\"page\" class=\"page-number\"\n           :class=\"{ active: page - 1 === currentPage }\"\n           @click=\"goToPage(page - 1)\">\n        {{ page }}\n      </div>\n      <div @click=\"nextPage\" class=\"button\"\n           :class=\"{ disabled: (currentPage + 1) * partiesPerPage >= parties.length }\">\n        Next page\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PartyAdapter from '../adapters/PartyAdapter.js';\n\nexport default {\n  name: \"AllPartiesComponent\",\n  data() {\n    return {\n      title: 'title',\n      currentPage: 0,\n      partiesPerPage: 9,\n      parties: [],\n      totalPages: 0,\n      url: \"http://localhost:8084\",\n    };\n  },\n  methods: {\n    redirectToPartyDetail(party) {\n      this.$router.push({\n        name: 'PartyDetail',\n        params: {partyId: party.id},\n        query: {description: party.description, importantPoints: JSON.stringify(party.importantPoints)},\n      });\n    },\n    async goToPage(page) {\n      if (page >= 0 && page < this.totalPages) {\n        this.currentPage = page;\n        await this.loadParties();\n      }\n    },\n    async nextPage() {\n      if (this.currentPage + 1 < this.totalPages) {\n        this.currentPage++;\n        await this.loadParties();\n      }\n\n    },\n    async previousPage() {\n      if (this.currentPage > 0) {\n        this.currentPage--;\n        await this.loadParties();\n      }\n    },\n\n    async loadParties() {\n      const adapter = new PartyAdapter(this.url);\n      const response = await adapter.getParties(this.currentPage, this.partiesPerPage);\n      this.parties = response.parties;\n      this.totalPages = response.totalPages;\n\n    },\n  },\n  async mounted() {\n    await this.loadParties();\n  },\n};\n</script>\n<style scoped>\n\n.container {\n  display: flex;\n  align-items: center;\n  flex-direction: column;\n  margin: 10% auto;\n  width: 50%;\n  height: 100%;\n\n}\n\n.intro {\n  display: flex;\n  flex-direction: column;\n  color: #17003C;\n  width: 80%;\n}\n\n\n.text {\n  display: flex;\n  margin-top: 1%;\n  color: #17003C;\n  font-size: 2vh;\n}\n\n.party-container {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n\n.party-box {\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: column;\n  align-items: center;\n  border-radius: 8%;\n  height: 26vh;\n  width: 20vh;\n  margin: 3%;\n  background-color: #efefef;\n  color: white;\n  border: solid #afafaf .2rem;\n  box-shadow: rgba(50, 50, 93, 0.25) 0px 13px 27px -5px, rgba(0, 0, 0, 0.3) 0px 8px 16px -8px;\n\n\n}\n\n.button {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  color: white;\n  background-color: #FF9900;\n  border: 0.3vh #9F6000 solid;\n  border-radius: 25px;\n  height: 12%;\n  width: 70%;\n  font-size: 1.5vh;\n  font-weight: 500;\n  text-transform: uppercase;\n  transition: background-color 0.3s ease, border-color 0.3s ease;\n  cursor: pointer;\n}\n\n.button:hover {\n  background-color: #9F6000;\n  border: 0.3vh #FF9900 solid;\n}\n\n.image {\n  height: 7vh;\n  width: 100%;\n  margin-top: 30%;\n  margin-bottom: 30%;\n  object-fit: contain;\n}\n\n.navigation {\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  width: 20vw;\n  height: 30vh;\n}\n\n.page-number {\n  margin: 5px 5px;\n  cursor: pointer;\n  transition: color 0.3s, color 0.3s;\n}\n\n.page-number:hover {\n  color: #FF9900;\n}\n\n.page-number.active {\n  color: #000000;\n  font-weight: bold;\n}\n\n* {\n  font-family: \"Outfit\", sans-serif;\n  font-optical-sizing: auto;\n  font-style: normal;\n}\n</style>"],"mappings":";AAsCA,OAAOA,YAAW,MAAO,6BAA6B;AAEtD,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,OAAO;MACdC,WAAW,EAAE,CAAC;MACdC,cAAc,EAAE,CAAC;MACjBC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,CAAC;MACbC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,qBAAqBA,CAACC,KAAK,EAAE;MAC3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAChBZ,IAAI,EAAE,aAAa;QACnBa,MAAM,EAAE;UAACC,OAAO,EAAEJ,KAAK,CAACK;QAAE,CAAC;QAC3BC,KAAK,EAAE;UAACC,WAAW,EAAEP,KAAK,CAACO,WAAW;UAAEC,eAAe,EAAEC,IAAI,CAACC,SAAS,CAACV,KAAK,CAACQ,eAAe;QAAC;MAChG,CAAC,CAAC;IACJ,CAAC;IACD,MAAMG,QAAQA,CAACC,IAAI,EAAE;MACnB,IAAIA,IAAG,IAAK,KAAKA,IAAG,GAAI,IAAI,CAAChB,UAAU,EAAE;QACvC,IAAI,CAACH,WAAU,GAAImB,IAAI;QACvB,MAAM,IAAI,CAACC,WAAW,CAAC,CAAC;MAC1B;IACF,CAAC;IACD,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,IAAI,CAACrB,WAAU,GAAI,IAAI,IAAI,CAACG,UAAU,EAAE;QAC1C,IAAI,CAACH,WAAW,EAAE;QAClB,MAAM,IAAI,CAACoB,WAAW,CAAC,CAAC;MAC1B;IAEF,CAAC;IACD,MAAME,YAAYA,CAAA,EAAG;MACnB,IAAI,IAAI,CAACtB,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;QAClB,MAAM,IAAI,CAACoB,WAAW,CAAC,CAAC;MAC1B;IACF,CAAC;IAED,MAAMA,WAAWA,CAAA,EAAG;MAClB,MAAMG,OAAM,GAAI,IAAI3B,YAAY,CAAC,IAAI,CAACQ,GAAG,CAAC;MAC1C,MAAMoB,QAAO,GAAI,MAAMD,OAAO,CAACE,UAAU,CAAC,IAAI,CAACzB,WAAW,EAAE,IAAI,CAACC,cAAc,CAAC;MAChF,IAAI,CAACC,OAAM,GAAIsB,