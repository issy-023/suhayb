ggedIn()\">\n        <div class=\"dropdown\">\n          <button class=\"dropbtn\">Profile</button>\n          <div class=\"dropdown-content\"><a @click=\"$router.push('/profile')\">Profile</a> <a v-if=\"isAdmin()\"\n                                                                                            @click=\"$router.push('/admin')\">Admin</a>\n            <a @click=\"logOut()\">Logout</a></div>\n        </div>\n      </div>\n    </div>\n    <div id=\"body\">\n      <div class=\"header-container\"><h1>Userlist</h1>        <input v-model=\"searchTerm\" type=\"text\"\n                                                                    placeholder=\"Search users by email...\"\n                                                                    class=\"search-input\"/></div>\n      <div class=\"table__body\">\n        <table>\n          <thead>\n          <tr>\n            <th> Id</th>\n            <th> Name</th>\n            <th> Email</th>\n            <th> Account status</th>\n            <th> role</th>\n\n          </tr>\n          </thead>\n          <tbody>\n          <tr v-for=\"user in filteredUsers\" :key=\"user.id\">\n            <td>{{ user.id }}</td>\n            <td>{{ user.username }}</td>\n            <td>{{ user.email }}</td>\n            <td>\n              <button :id=\"'button-' + user.id\" :class=\"{'active-btn': user.active, 'inactive-btn': !user.active}\"\n                      @click=\"toggleUserStatus(user.id, user.active)\">\n                {{ user.active ? 'Active' : 'Deactivate' }}\n              </button>\n              <button @click=\"deleteUser(user.id)\" class=\"delete-btn\">Delete</button>\n            </td>\n            <td>\n              <select v-model=\"user.role\" @change=\"updateUserRole(user.id, user.role)\">\n                <option value=\"user\">User</option>\n                <option value=\"admin\">Admin</option>\n              </select>\n            </td>\n          </tr>\n          <tr v-if=\"filteredUsers.length === 0\">\n            <td colspan=\"5\" style=\"text-align: center;\">No users found</td>\n          </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n\n</template>\n<script>\nimport axios from 'axios';\nimport { API_URL } from '@/config';\n\nexport default {\n  name: 'AdminPage',\n  data() {\n    return {\n      users: [],\n      searchTerm: '',\n    };\n  },\n  computed: {\n    filteredUsers() {\n      return this.users.filter(user =>\n          user.email.toLowerCase().includes(this.searchTerm.toLowerCase())\n      );\n    },\n  },\n  methods: {\n    isLoggedIn() {\n      return !!localStorage.getItem('token');\n    },\n    isAdmin() {\n      return !!localStorage.getItem('isAdmin');\n    },\n    logOut() {\n      localStorage.removeItem('token');\n      localStorage.removeItem('isAdmin');\n      this.$router.push('/');\n    },\n    async fetchUsers() {\n      try {\n        const token = localStorage.getItem('token');\n        const response = await axios.get(`${API_URL}/admin/users`, {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        this.users = response.data;\n      } catch (error) {\n        if (error.response && error.response.status === 401) {\n          this.logOut(); // Log out and redirect to login if unauthorized\n          window.alert('Unauthorized access, please log in again.');\n        } else {\n          console.error('Error fetching users:', error);\n        }\n      }\n    },\n    async toggleUserStatus(userId, isActive) {\n      const token = localStorage.getItem('token');\n      const adminId = localStorage.getItem('userId');\n      try {\n        await axios.put(`${API_URL}/user/${userId}/status`, null, {\n          params: { isActive: !isActive },\n          headers: {\n            'Content-Type': 'application/json',\n            'Authorization': `Bearer ${token}`,\n            'UserId': adminId,\n          },\n        });\n        const user = this.users.find(u => u.id === userId);\n        if (user) {\n          user.active = !isActive;\n        }\n      } catch (error) {\n        if (error.response && error.response.status === 403) {\n          window.alert('You cannot deactivate your own account.');\n        } else if (error.response && error.response.status === 401) {\n          this.logOut();\n          window.alert('Unauthorized access, please log in again.');\n        } else {\n          console.error('Error updating user status:', error);\n        }\n      }\n    },\n    async deleteUser(id) { // Changed from userId to username\n      const token = localStorage.getItem('token');\n      try {\n        await axios.delete(`${API_URL}/admin/user/${id}`, { // Updated to use username\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        // Remove the user from the list after deletion\n         // Also use username here\n      } catch (error) {\n        console.error('Error deleting user:', error);\n        window.alert('An error occurred while trying to delete the user.');\n      }\n    },\n    async updateUserRole(userId, newRole) {\n      const token = localStorage.getItem('token');\n      try {\n        await axios.put(`${API_URL}/user/${userId}/role`, { role: newRole }, {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        const user = this.users.find(u => u.id === userId);\n        if (user) {\n          user.role = newRole;\n        }\n      } catch (error) {\n        console.error('Error updating user role:', error);\n        window.alert('An error occurred while trying to update the user role.');\n      }\n    }\n\n\n  },\n  mounted() {\n    if (!this.isLoggedIn()) {\n      this.$router.push('/');\n      window.alert('Please log in to access the admin page.');\n    } else {\n      this.fetchUsers();\n    }\n  },\n};\n</script>\n\n\n<style scoped>\n.parent {\n  display: grid;\n  grid-template-rows: 0.2fr 1fr;\n  grid-template-columns: 0.5fr 1fr 1fr;\n  height: 100vh;\n}\nh1{\n  color: white;\n}\n\n#left {\n  background-color: #17003C;\n  grid-row: 2 / 4;\n  grid-column: 1 / 2;\n  padding-left: 2rem;\n}\n\n#nav {\n  background-color: #17003C;\n  grid-row: 1 / 2;\n  grid-column: 1 / 4;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 2rem;\n  color: white;\n}\n\n#body {\n  grid-row: 3/ 4;\n  grid-column:  3/ 4;\n  padding: 1rem;\n}\n\n#footer {\n  margin: 0;\n}\n\n.btn {\n  width: 90%;\n  height: 5%;\n}\n\n.dropdown {\n  position: relative;\n  display: inline-block;\n}\n\n.dropdown-content {\n  display: none;\n  position: absolute;\n  background-color: white;\n  min-width: 160px;\n}\n\n.dropdown-content a {\n  color: black;\n  padding: 12px 16px;\n  text-decoration: none;\n  display: block;\n}\n\n.dropdown-content a:hover {\n  background-color: #1D4BF2;\n  color: white;\n  cursor: pointer;\n}\n\n.dropdown:hover .dropdown-content {\n  display: block;\n}\n\n.table__body {\n  width: 95%;\n  max-height: 80%;\n  background-color: #fffb;\n  margin: .8rem auto;\n  border-radius: .6rem;\n  overflow-y: auto;\n}\n\ntable {\n  width: 100%;\n}\n\ntable, th, td {\n  border-collapse: collapse;\n  padding: 1rem;\n  text-align: left;\n}\n\nthead th {\n  position: sticky;\n  top: 0;\n  background-color: #d5d1defe;\n  z-index: 1;\n}\n\ntbody tr:nth-child(even) {\n  background-color: #0000000b;\n}\n\n#button {\n  width: 80%;\n}\n\n.header-container {\n  display: flex;\n  align-items: center;\n  margin-bottom: 1rem;\n}\n\n.search-input {\n  max-width: 20%;\n  padding: 0.5rem;\n  margin-left: 45rem;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\n.active-btn {\n  background-color: blue;\n  color: white;\n  border: none;\n  padding: 5px 10px;\n  margin-left: 5px;\n  cursor: pointer;\n  border-radius: 4px;\n}\n\n.inactive-btn {\n  background-color: grey;\n  color: white;\n  border: none;\n  padding: 5px 10px;\n  margin-left: 5px;\n  cursor: pointer;\n  border-radius: 4px;\n}\n  .delete-btn {\n    background-color: red;\n    color: white;\n    border: none;\n    padding: 5px 10px;\n    margin-left: 5px;\n    cursor: pointer;\n    border-radius: 4px;\n  }\n  .delete-btn:hover {\n    background-color: darkred;\n  }\n</style>"],"mappings":";;;EAEOA,KAAK,EAAC;AAAQ;;EACZC,EAAE,EAAC;AAAK;;EAHjBC,GAAA;EAIWF,KAAK,EAAC;;;EACJA,KAAK,EAAC;AAAU;;EAEdA,KAAK,EAAC;AAAkB;;EAM9BC,EAAE,EAAC;AAAM;;EACPD,KAAK,EAAC;AAAkB;;EAGxBA,KAAK,EAAC;AAAa;mBAjB9B;oBAAA;oBAAA;;EAAAE,GAAA;AAAA;;uBAEEC,mBAAA,CAqDM,OArDNC,UAqDM,GApDJC,mBAAA,CASM,OATNC,UASM,G,0BATQD,mBAAA,CAAc,YAAV,OAAK,sBACSE,QAAA,CAAAC,UAAU,M,cAAxCL,mBAAA,CAOM,OAPNM,UAOM,GANJJ,mBAAA,CAKM,OALNK,UAKM,G,0BAJJL,mBAAA,CAAwC;IAAhCL,KAAK,EAAC;EAAS,GAAC,SAAO,sBAC/BK,mBAAA,CAEuC,OAFvCM,UAEuC,GAFTN,mBAAA,CAAgD;IAA5CO,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,IAAA,CAAAC,OAAO,CAACC,IAAI;KAAc,SAAO,G,0BAPpFC,gBAAA,KAOkGX,QAAA,CAAAY,OAAO,M,cAAhBhB,mBAAA,CAC4C;IARrID,GAAA;IAQ6FU,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,IAAA,CAAAC,OAAO,CAACC,IAAI;KAAY,OAAK,KARjIG,mBAAA,gBASYf,mBAAA,CAA+B;IAA3BO,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEP,QAAA,CAAAc,MAAM;KAAI,QAAM,E,SATvCD,mBAAA,e,GAaIf,mBAAA,CAyCM,OAzCNiB,UAyCM,GAxCJjB,mBAAA,CAE0F,OAF1FkB,UAE0F,G,0BAF5DlB,mBAAA,CAAiB,YAAb,UAAQ,sB,0BAdhDa,gBAAA,K,gBAc6Db,mBAAA,CAE6B;IAhB1F,uBAAAQ,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAc6EU,KAAA,CAAAC,UAAU,GAAAX,MAAA;IAAEY,IAAI,EAAC,MAAM;IAChCC,WAAW,EAAC,0BAA0B;IACtC3B,KAAK,EAAC;iDAFGwB,KAAA,CAAAC,UAAU,E,KAGjFpB,mBAAA,CAoCM,OApCNuB,UAoCM,GAnCJvB,mBAAA,CAkCQ,gB,4BAjCNA,mBAAA,CASQ,gBARRA,mBAAA,CAOK,aANHA,mBAAA,CAAY,YAAR,KAAG,GACPA,mBAAA,CAAc,YAAV,OAAK,GACTA,mBAAA,CAAe,YAAX,QAAM,GACVA,mBAAA,CAAwB,YAApB,iBAAe,GACnBA,mBAAA,CAAc,YAAV,OAAK,E,wBAIXA,mBAAA,CAsBQ,iB,kBArBRF,mBAAA,CAiBK0B,SAAA,QA/CfC,WAAA,CA8B6BvB,QAAA,CAAAwB,aAAa,EAArBC,IAAI;yBAAf7B,mBAAA,CAiBK;MAjB8BD,GAAG,EAAE8B,IAAI,CAAC/B;QAC3CI,mBAAA,CAAsB,YAAA4B,gBAAA,CAAfD,IAAI,CAAC/B,EAAE,kBACdI,mBAAA,CAA4B,YAAA4B,gBAAA,CAArBD,IAAI,CAACE,QAAQ,kBACpB7B,mBAAA,CAAyB,Y